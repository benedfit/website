{
  "name": "benedfit.com",
  "version": "3.0.0",
  "description": "Personal website of Ben Edwards, a Web Designer and Developer",
  "private": true,
  "scripts": {
    "start": "pliers go -o",
    "build": "pliers build",
    "lint": "yarn lint:css && yarn lint:js && yarn lint:pug",
    "lint:css": "csslint . --quiet",
    "lint:js": "jshint",
    "lint:pug": "pug-lint .",
    "pretest": "pliers build && yarn lint",
    "test": "istanbul --include-all-sources cover mocha -- --recursive -R spec",
    "posttest": "istanbul check-coverage && rm -rf coverage"
  },
  "dependencies": {
    "any-newer-files": "0.0.4",
    "async": "3.2.2",
    "autoprefixer-stylus": "1.0.0",
    "browser-sync": "2.27.7",
    "clean-css": "5.2.2",
    "cloudflare-cli": "4.2.0",
    "configury": "0.3.0",
    "front-matter": "4.0.2",
    "fs-extra": "10.0.0",
    "glob": "7.2.0",
    "html-minifier": "4.0.0",
    "jade": "1.11.0",
    "jstransformer-uglify-js": "1.2.0",
    "lodash.merge": "4.6.2",
    "mkdirp": "1.0.4",
    "moment": "2.29.1",
    "namp": "0.2.26",
    "rfg-api": "0.5.2",
    "rimraf": "3.0.2",
    "stylus": "0.55.0",
    "stylus-mixins": "0.4.0",
    "stylus-renderer": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@newhighsco/commitlint-config": "1.0.18",
    "@newhighsco/editor-config": "1.1.2",
    "csslint": "1.0.5",
    "husky": "7.0.4",
    "istanbul": "0.4.5",
    "jshint": "2.13.1",
    "mocha": "9.1.3",
    "pliers": "1.2.1",
    "pliers-clean-shrinkwrap": "1.0.1",
    "pliers-imagemin": "3.0.2",
    "pliers-npm-security-check": "1.0.0",
    "pug-lint": "2.6.0",
    "pug-lint-config-clock": "2.0.0"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ],
    "reviewers": [
      "@benedfit"
    ]
  }
}
